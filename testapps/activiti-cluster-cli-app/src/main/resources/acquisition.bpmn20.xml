    <sequenceFlow id="flow4" sourceRef="exclusivegateway1" targetRef="usertask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outcome == 'A'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask4" name="Select suppliers" activiti:assignee="John Connor"></userTask>
    <sequenceFlow id="flow5" sourceRef="exclusivegateway1" targetRef="usertask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outcome == 'B'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask5" name="Request quote from supplier" activiti:assignee="Sarah Connor">
      <multiInstanceLoopCharacteristics isSequential="false">
        <loopCardinality>5</loopCardinality>
      </multiInstanceLoopCharacteristics>
    </userTask>
    <sequenceFlow id="flow6" sourceRef="usertask4" targetRef="usertask5"></sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="usertask5" targetRef="servicetask1"></sequenceFlow>
    <serviceTask id="servicetask1" name="Calculate optimal offer" activiti:class="com.activiti.DummyDelegate"></serviceTask>
    <boundaryEvent id="boundarytimer1" name="Timer" attachedToRef="usertask5" cancelActivity="true">
      <timerEventDefinition>
        <timeDuration>PT5M</timeDuration>
      </timerEventDefinition>
    </boundaryEvent>
    <sequenceFlow id="flow8" sourceRef="boundarytimer1" targetRef="servicetask1"></sequenceFlow>
    <userTask id="usertask6" name="Send result to requester" activiti:assignee="Kyle Reese"></userTask>
    <sequenceFlow id="flow9" sourceRef="servicetask1" targetRef="usertask6"></sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="usertask3" targetRef="servicetask2"></sequenceFlow>
    <serviceTask id="servicetask2" name="Record request information" activiti:class="com.activiti.DummyDelegate"></serviceTask>
    <sequenceFlow id="flow11" sourceRef="usertask6" targetRef="servicetask2"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow12" sourceRef="servicetask2" targetRef="endevent1"></sequenceFlow>
